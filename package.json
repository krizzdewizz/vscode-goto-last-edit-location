{
	"name": "goto-last-edit-location",
	"description": "Visual Studio Code extension that provides the 'Goto last edit location' command",
	"version": "0.3.0",
	"publisher": "krizzdewizz",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"bugs": {
		"url": "https://github.com/krizzdewizz/vscode-goto-last-edit-location/issues"
	},
	"homepage": "https://github.com/krizzdewizz/vscode-goto-last-edit-location",
	"repository": {
		"type": "git",
		"url": "https://github.com/krizzdewizz/vscode-goto-last-edit-location.git"
	},
	"activationEvents": [
		"*"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.gotoPreviousEditLocation",
				"title": "Goto previous edit location"
			},
			{
				"command": "extension.gotoNextEditLocation",
				"title": "Goto next edit location"
			},
			{
				"command": "extension.clearLocationHistory",
				"title": "Clear location history"
			}
		],
		"keybindings": [
			{
				"command": "extension.gotoPreviousEditLocation",
				"key": "ctrl+q",
				"mac": "ctrl+q"
			},
			{
				"command": "extension.gotoNextEditLocation",
				"key": "ctrl+shift+q",
				"mac": "ctrl+shift+q"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile --watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x",
		"tslint": "*"
	}
}